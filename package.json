{
  "name": "aoo-cwa-init",
  "version": "0.0.1",
  "description": "CWA INIT ",
  "main": "index.js",
  "files": [
    "index.js",
    "dist",
    ".babelrc",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "unit": "npm test",
    "start": "webpack-dev-server --config webpack.config.demo.js",
    "test": "jest --coverage",
    "lint": "eslint 'src/**/*.js' 'demo/**/*.js' 'src/**/*.jsx' 'demo/**/*.jsx'",
    "lint:fix": "eslint --fix 'src/**/*.js' 'demo/**/*.js' 'src/**/*.jsx' 'demo/**/*.jsx'",
    "prepush": "npm run lint && npm t",
    "precommit": "lint-staged",
    "validate": "npm ls",
    "icon": "fontcustom compile"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ],
    "src/**/*.jsx": [
      "npm run lint"
    ],
    "demo/**/*.js": [
      "npm run lint"
    ],
    "demo/**/*.jsx": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://digital-gerrit.service.test.group/client-web-application/core"
  },
  "contributors": [
    {
      "name": "Shaun Wallace",
      "email": "swallace@sapient.com"
    },
    {
      "name": "Paul Pruteanu",
      "email": "paul.pruteanu@lloydsbanking.com"
    }
  ],
  "private": false,
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "0.16.2",
    "Q": "1.0.0",
    "deep-diff": "0.3.8",
    "immutable": "3.8.1",
    "keymirror": "0.1.1",
    "react": "0.13.3"
  },
  "peerDependencies": {
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-stage-3": "6.24.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.24.1",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.24.1",
    "enzyme": "^2.8.2",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "3.19.0",
    "eslint-config-canonical": "8.2.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.28.0",
    "husky": "0.13.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "lint-staged": "4.0.2",
    "prettier": "1.3.1",
    "raw-loader": "0.5.1",
    "react-demo": "1.5.0",
    "react-demo-library": "0.1.7",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5",
    "webpack-node-externals": "1.6.0"
  },
  "engines": {
    "npm": "^3.0.0",
    "node": "^6.10.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "/actions",
      "/assets",
      "/config/index.js",
      "/dispatcher",
      "/mockdata",
      "/stores",
      "/utils"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "timers": "fake"
  },
  "nyc": {
    "exclude": [
      "**/test.js",
      "**/style.js",
      "**/*.spec.js",
      "**/stub.js",
      "**/story.jsx",
      "**/index.js"
    ],
    "extension": [
      ".jsx",
      ".js"
    ],
    "report-dir": "reports/coverage/phantomjs-lloyds-retail",
    "reporter": [
      "lcov",
      "cobertura"
    ]
  }
}
